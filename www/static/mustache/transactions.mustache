<table>
	<tr>
		<th>ITEMS</th>
		<th>PLACE</th>
		<th>AMOUNT</th>
		<th>DATE</th>
		<th>PAID BY</th>
		<th>ACCOUNT</th>
	</tr>
	{{#transactions}}
	<tr>
		<td><input type="text" value="{{items}}"   class="items"   /></td>
		<td><input type="text" value="{{place}}"   class="place"   /></td>
		<td><input type="text" value="{{amount_string}}"  class="amount"  /></td>
		<td><input type="text" value="{{date}}"    class="date"    /></td>
		<td><input type="text" value="{{paid_by_obj.first_name}} {{paid_by_obj.last_name}}" class="paid_by" /></td>
		<td><input type="text" value="{{account_obj.name}}" class="account" /></td>
	</tr>
	{{/transactions}}
</table>
